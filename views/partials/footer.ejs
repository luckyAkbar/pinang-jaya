<footer>
  <div class="container-fluid p-5">
    <div class="row">
      <div class="col-md-2">
        <h6 class="font-weight-bold mb-3">Kontak</h6>
        <ul>
          <li class="text-secondary"><%= villageName %></li>
          <li class="text-secondary"><%= districtName %></li>
          <li class="text-secondary"><%= provinceName %>, <%= postalCode %></li>
          <li class="text-secondary"><%= phoneContact %></li>
          <li class="text-secondary"><%= villageEmailAddress %></li>
        </ul>
      </div>

      <% footerLinks.forEach((link) => { %>
        <div class="col-md-2">
          <h6 class="font-weight-bold mb-3"><%= link.mainLink %></h6>
          <ul>
            <% link.subLinks.forEach((subLink) => { %>
              <li><a href="<%= subLink.href %>"><%= subLink.title %></a></li>
            <% }) %>
          </ul>
        </div>
      <% }); %>
    </div>
  </div>

  <div class="container-fluid">
    <div class="mt-3">
      <!-- <div class="row mt-3"> -->
      <div class="float-left">
        <!-- <hr class="float-left" style="width:40%;"> -->
        <p class="">&copy <%= copyrightYear %> <%= villageName %> <%= districtName %> </p>
        <!-- <hr class="float-left" style="width:25%;"> -->
      </div>

      <div class="float-right social-icon">
        <a href=""><i class="fab fa-facebook-square"></i></a>
        <a href=""><i class="fab fa-twitter-square"></i></a>
        <a href=""><i class="fab fa-linkedin"></i></a>
        <a href=""><i class="fab fa-youtube"></i></a>
        <a href=""><i class="fab fa-instagram"></i></a>
      </div>
    </div>
    <div class="clearfix"></div>
  </div>
  </div>
</footer>